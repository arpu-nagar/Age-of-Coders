<body>
  <script type="text/javascript">
    window.addEventListener("load", () => {
      fetch(window.location.href + "/message", {
          method: "GET"
        })
        .then(res => {
                return res.json();
        })
        .then(function (json) {
          if(json.contestend===true){
            alert("Contest has ended");
          }
        });
    });
  </script>
  <div class="main-container">
    <div class="top-container">
      <img src="/logos/fullnew.svg" alt="" class='iecse_logo'>
      <img src="/logos/Group 28AOC.png" class="aoc-logo" />
      <div class="btn-div">
        <a href="/regions"><button class="btn">RETURN</button></a>
      </div>
    </div>
    <div class="table_container">
      <table class="lead">
        <thead>
          <tr class="headrow">
            <th>USERNAME</th>
            <th>POINTS</th>
            <th>RANK</th>
          </tr>
        </thead>
        <tbody>
          <% result.forEach(function(row){ %>
          <tr>
            <td><%= row.username %></td>
            <td><%= row.score %></td>
            <td><%= row.rank %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>

  </div>
  <div class="mobile">
    <div class="mob_content">Dear user, <br>
      please open the Age of Coders website on your laptop, so that you can enjoy a pleasant and seamless experience.
      <br><br>
      Thank You,
      <br>
      Team IECSE</div>
  </div>
