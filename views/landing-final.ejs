          <div class="area">
            <img src="/logos/Arect.png" class="rect" />
            <div class="reg-name">
              <span> <%=regions[0].rname%></span>
            </div>
          </div>
        </a>
        <img class="ellipse A" src="/logos/Alower.svg" />

        <!-- <div class="holder-name">
